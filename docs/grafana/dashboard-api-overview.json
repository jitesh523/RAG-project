{
  "title": "RAG Aerospace API Overview",
  "timezone": "browser",
  "panels": [
    {
      "type": "graph",
      "title": "HTTP Request Duration (ms)",
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))"}
      ]
    },
    {
      "type": "graph",
      "title": "HTTP Requests per Second",
      "targets": [
        {"expr": "sum(rate(starlette_requests_total[1m]))"}
      ]
    },
    {
      "type": "graph",
      "title": "Errors per Second",
      "targets": [
        {"expr": "sum(rate(starlette_requests_total{status=~\"5..\"}[1m]))"}
      ]
    },
    {
      "type": "graph",
      "title": "Vector Search 95th Latency (ms)",
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(vector_search_duration_seconds_bucket[5m])) by (le))"}
      ]
    },
    {
      "type": "graph",
      "title": "Ingestion Throughput (docs/min)",
      "targets": [
        {"expr": "sum(rate(ingest_documents_total[1m])) * 60"}
      ]
    },
    {
      "type": "graph",
      "title": "Rate Limited (429/s)",
      "targets": [
        {"expr": "sum(rate(starlette_requests_total{status=\"429\"}[1m]))"}
      ]
    },
    {
      "type": "graph",
      "title": "Ask Retries (count)",
      "targets": [
        {"expr": "sum(ask_retries_total)"}
      ]
    },
    {
      "type": "graph",
      "title": "Vector Search Retries (by backend)",
      "targets": [
        {"expr": "sum by (backend) (vector_search_retries_total)"}
      ]
    },
    {
      "type": "graph",
      "title": "Ingest Retries (embed vs insert)",
      "targets": [
        {"expr": "sum by (stage) (ingest_retries_total)"}
      ]
    },
    {
      "type": "graph",
      "title": "Quota Usage by Tenant",
      "targets": [
        {"expr": "sum by (tenant) (ask_usage_total)"}
      ]
    },
    {
      "type": "graph",
      "title": "Embedding Batches (count)",
      "targets": [
        {"expr": "sum(embed_batches_total)"}
      ]
    },
    {
      "type": "graph",
      "title": "Embedding Items (count)",
      "targets": [
        {"expr": "sum(embed_items_total)"}
      ]
    },
    {
      "type": "graph",
      "title": "Embed Batch Duration p95 (s)",
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(embed_batch_duration_seconds_bucket[5m])) by (le))"}
      ]
    },
    {
      "type": "stat",
      "title": "Circuit State (LLM)",
      "targets": [
        {"expr": "max(circuit_state{component='llm'})"}
      ]
    },
    {
      "type": "graph",
      "title": "Semantic Cache Hit Ratio by Tenant",
      "targets": [
        {"expr": "sum(rate(semantic_cache_hits_total[5m])) by (tenant) / (sum(rate(semantic_cache_hits_total[5m])) by (tenant) + sum(rate(semantic_cache_misses_total[5m])) by (tenant))"}
      ],
      "yaxes": [{"format": "percent"}, {"format": "short"}]
    },
    {
      "type": "graph",
      "title": "LLM Timeouts (504/s)",
      "targets": [
        {"expr": "sum(rate(starlette_requests_total{status=\"504\"}[5m]))"}
      ]
    },
    {
      "type": "graph",
      "title": "Ask LLM Latency p95 (s)",
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(ask_llm_duration_seconds_bucket[5m])) by (le))"}
      ]
    },
    {
      "type": "graph",
      "title": "Ask Usage by Tenant (rps)",
      "targets": [
        {"expr": "sum by (tenant) (rate(ask_usage_total[5m]))"}
      ]
    },
    {
      "type": "graph",
      "title": "LLM Provider Latency p95 (s) by Provider",
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(provider_llm_latency_seconds_bucket[5m])) by (provider,le))"}
      ]
    },
    {
      "type": "graph",
      "title": "HITL Enqueued by Tenant (rate)",
      "targets": [
        {"expr": "sum by (tenant,reason) (rate(hitl_enqueued_total[5m]))"}
      ]
    },
    {
      "type": "graph",
      "title": "Guardrail Blocks (input)",
      "targets": [
        {"expr": "sum(rate(starlette_requests_total{path='/ask',status='400'}[5m]))"}
      ]
    },
    {
      "type": "graph",
      "title": "LLM Circuit State History",
      "targets": [
        {"expr": "max_over_time(circuit_state{component='llm'}[5m])"}
      ]
    },
    {
      "type": "graph",
      "title": "Cost USD by Tenant",
      "targets": [
        {"expr": "sum by (tenant) (rate(cost_usd_total[5m]))"}
      ]
    }
  ],
  "templating": {"list": []},
  "annotations": {"list": []},
  "time": {"from": "now-6h", "to": "now"}
}
